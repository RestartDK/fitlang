program             = { statement } ;

statement           = declaration | assignment | function_call | control_structure ;

declaration         = "ClientProfile" | "WorkoutPlan", identifier ;

assignment          = identifier, ".", identifier, "=", value ;

function_call       = identifier, "(", [ argument_list ], ")" ;

control_structure   = if_statement | foreach_statement | try_catch_block ;

if_statement        = "If", "(", expression, ")", block, [ "Else", block ] ;

foreach_statement   = "ForEach", "(", identifier, "in", identifier, ")", block ;

try_catch_block     = "Try", block, "Catch", "(", identifier, ")", block ;

block               = "{", program, "}" | statement ;

argument_list       = expression, { ",", expression } ;

expression          = simple_expression | string | time_expression ;

simple_expression   = identifier | number ;

string              = "\"", { char }, "\"" ;

time_expression     = number, ":", number ;

number              = integer | float ;

integer             = digit, { digit } ;

float               = digit, { digit }, ".", { digit } ;

identifier          = letter, { letter | digit | "_" } ;

value               = string | number | array | object ;

array               = "[", [ value, { ",", value } ], "]" ;

object              = "{", [ pair, { ",", pair } ], "}" ;

pair                = string, ":", value ;

letter              = "A" | ... | "Z" | "a" | ... | "z" ;

digit               = "0" | ... | "9" ;

char                = letter | digit | symbol | escape_sequence ;

symbol              = " " | "!" | "#" | "$" | "%" | "&" | "'" | "(" | ")" | "*" | "+" | "," | "-" | "." | "/" | ":" | ";" | "<" | "=" | ">" | "?" | "@" | "[" | "\" | "]" | "^" | "_" | "`" | "{" | "|" | "}" | "~" ;

escape_sequence     = "\", ( "n" | "t" | "r" | "\"" | "'" | "\\" ) ;
